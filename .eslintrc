{
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react",
    "react-app"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "unicorn",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "global-require": 0,
    "no-use-before-define": 0,
    "react/destructuring-assignment": 0,
    "react/jsx-filename-extension": [2, {"extensions": [".js"]}],
    "import/no-cycle": 2,
    "import/prefer-default-export": 1,
    "unicorn/import-index": 2
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "globals": {
    "__DEV__": true,
    "__IS_TEST_RUN__": true
  }
}
